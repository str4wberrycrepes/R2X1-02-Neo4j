<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:ex="http://example.org/ontology#">

    <owl:Class rdf:about="http://example.org/ontology#COMPUTER_SCIENCE_000001">
        <rdfs:label>Computer Science</rdfs:label>
        <rdfs:comment>Root discipline</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/ontology#AI_000002">
        <rdfs:label>Artificial Intelligence</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://example.org/ontology#COMPUTER_SCIENCE_000001"/>
        <rdfs:comment>KEYWORD</rdfs:comment>
        </owl:Class>

    <owl:Class rdf:about="http://example.org/ontology#ML_000003">
        <rdfs:label>Machine Learning</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://example.org/ontology#AI_000002"/>
        <ex:relationshipStrength>0.9</ex:relationshipStrength>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/ontology#NLP_000004">
        <rdfs:label>Natural Language Processing</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://example.org/ontology#AI_000002"/>
        <rdfs:comment>KEYWORD</rdfs:comment>
        <ex:relationshipStrength>0.85</ex:relationshipStrength>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/ontology#DataStructures_000005">
        <rdfs:label>Data Structures</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://example.org/ontology#COMPUTER_SCIENCE_000001"/>
    </owl:Class>

</rdf:RDF>
    